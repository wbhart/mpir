    norun)
      gmp_prog_cc_works="no, gcc-4.3.2 on 64-bit is bad , try -O1 or -fno-strict-aliasing for the flags, program does not run"
      ;;
  esac
fi



if test "$gmp_prog_cc_works" = yes; then
