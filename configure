    IFS="$ac_save_ifs"
    if test -z "$gmp_cv_prog_m4"; then
      as_fn_error $? "No usable m4 in \$PATH or /usr/5bin (see config.log for reasons)." "$LINENO" 5
    fi
  fi
  rm -f conftest.m4
fi
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $gmp_cv_prog_m4" >&5
$as_echo "$gmp_cv_prog_m4" >&6; }
